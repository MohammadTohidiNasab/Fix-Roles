@model dynamic

<h2>User Profile</h2>

<h3>User Claims</h3>
<table class="table">
    <thead>
        <tr>
            <th>Claim Type</th>
            <th>Claim Value</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var claim in Model.UserClaims)
        {
            <tr>
                <td>@claim.Type</td>
                <td>@claim.Value</td>
            </tr>
        }
    </tbody>
</table>

<h3>Session Data</h3>
<table class="table">
    <thead>
        <tr>
            <th>Session Key</th>
            <th>Session Value</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var session in Model.SessionData)
        {
            <tr>
                <td>@session.Key</td>
                <td>@session.Value</td>
            </tr>
        }
    </tbody>
</table>
